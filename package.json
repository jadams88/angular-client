{
  "name": "demo-angular-client",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Jonathon Adams"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jadams88/angular-client"
  },
  "homepage": "https://github.com/jadams88/angular-client#readme",
  "scripts": {
    "android": "tns run android --bundle",
    "ios": "tns run ios --bundle",
    "android:build": "tns build android --bundle",
    "ios:build": "tns build ios --bundle",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:push": "jest --ci=true --coverage --bail",
    "test:staged": "jest --ci=true --passWithNoTests --coverage --bail --findRelatedTests",
    "snapshot:update": "jest --updateSnapshot --testPathPattern",
    "test:ci": "jest --runInBand",
    "e2e:chrome": "testcafe -c 3 chrome e2e/src/ --r json:e2e/report.json",
    "e2e:firefox": "testcafe -c 3 firefox e2e/src/",
    "e2e:safari": "testcafe -c 3 safari e2e/src/",
    "e2e:edge": "testcafe edge e2e/src/",
    "format:fix": "pretty-quick --staged",
    "lint": "ng lint client",
    "lint:fix": "ng lint client --fix",
    "dev": "concurrently \"ng serve -o\" \"npm run server\"",
    "server": "node dev-server/index.js",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix lint-staged",
      "pre-push": "run-s format:fix"
    }
  },
  "lint-staged": {
    "linters": {
      "*.ts": [
        "tslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*.min.js"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.3",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "~7.2.3",
    "@angular/compiler": "~7.2.3",
    "@angular/core": "~7.2.3",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/forms": "~7.2.3",
    "@angular/http": "~7.2.3",
    "@angular/material": "^7.3.0",
    "@angular/platform-browser": "~7.2.3",
    "@angular/platform-browser-dynamic": "~7.2.3",
    "@angular/router": "~7.2.3",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/entity": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "apollo-angular": "^1.5.0",
    "apollo-angular-link-http": "^1.4.0",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-error": "^1.1.7",
    "core-js": "^2.6.3",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "nativescript-angular": "^7.2.0",
    "nativescript-localstorage": "^2.0.0",
    "nativescript-theme-core": "~1.0.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.4.0",
    "socket.io-client": "^2.1.1",
    "tns-core-modules": "^5.1.2",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular-devkit/core": "^7.3.0",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "~7.2.3",
    "@angular/language-service": "~7.2.3",
    "@nativescript/schematics": "^0.5.0",
    "@ngrx/schematics": "^7.2.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.21",
    "bcryptjs": "^2.4.3",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "jest-cli": "^24.0.0",
    "jest-marbles": "^2.2.0",
    "jest-preset-angular": "^6.0.1",
    "json-graphql-server": "^2.1.2",
    "json-server": "^0.14.2",
    "jsonwebtoken": "^8.4.0",
    "lint-staged": "^8.1.1",
    "nativescript-dev-sass": "^1.6.1",
    "nativescript-dev-typescript": "^0.7.9",
    "nativescript-dev-webpack": "^0.19.1",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "reset-css": "^4.0.1",
    "rxjs-tslint": "^0.1.5",
    "testcafe": "^0.23.3",
    "testcafe-angular-selectors": "^0.3.2",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "typescript": "~3.2.4"
  },
  "nativescript": {
    "id": "org.nativescript.client",
    "tns-ios": {
      "version": "5.1.0"
    },
    "tns-android": {
      "version": "5.1.0"
    }
  }
}
